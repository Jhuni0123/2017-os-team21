/dts-v1/;
#include "exynos3250.dtsi"

/ {
	model = "Samsung ARTIK5 board based on EXYNOS3250";
	compatible = "samsung,artik5", "samsung,exynos3250";

	chosen {
		bootargs = "console=ttySAC2,115200 vmalloc=256M ess_setup=0x46000000";
	};

	memory {
		reg = <0x40000000 0x1F800000>;
	};

	aliases {
		i2c8 = &i2c_gpio0;
	};

	fixed-rate-clocks {
		#address-cells = <1>;
		#size-cells = <0>;

		xrtcxti: clock@0 {
			compatible = "fixed-clock";
			reg = <0>;
			clock-frequency = <32768>;
			#clock-cells = <0>;
			clock-output-names = "xrtcxti";
		};

		xusbxti: clock@1 {
			compatible = "fixed-clock";
			clock-frequency = <24000000>;
			reg = <1>;
			#clock-cells = <0>;
			clock-output-names = "xusbxti";
		};

		xxti: clock@2 {
			compatible = "fixed-clock";
			reg = <2>;
			clock-frequency = <0>;
			#clock-cells = <0>;
			clock-output-names = "xxti";
			};

		xtcxo: clock@3 {
			compatible = "fixed-clock";
			reg = <3>;
			clock-frequency = <26000000>;
			#clock-cells = <0>;
			clock-output-names = "xtcxo";
		};

		audiocdclk: clock@4 {
			compatible = "fixed-clock";
			reg = <4>;
			clock-frequency = <0>;
			#clock-cells = <0>;
			clock-output-names = "audiocdclk";
		};
	};

	i2c@13860000 {
		status = "okay";
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		#size-cells = <0>;

		s2mps14_pmic@66 {
			compatible = "samsung,s2mps14-pmic";
			reg = <0x66>;
			interrupts = <5 0 0>;
			interrupt-parent = <&gpx3>;
			pinctrl-names = "default";
			pinctrl-0 = <&pmic_irq &pm_wrsti>;
			/* RTC: wtsr/smpl */
                        wtsr_en = <1>; /* enable */
                        smpl_en = <1>; /* enable */
                        wtsr_timer_val = <3>;   /* 1000ms */
                        smpl_timer_val = <4>;   /* 500ms */
                        check_jigon = <0>; /* do not check jigon */
                        /* RTC: If it's first boot, reset rtc to 1/1/2014 12:00:00(Wed) */
                        init_time,sec = <0>;
                        init_time,min = <0>;
                        init_time,hour = <12>;
                        init_time,mday = <1>;
                        init_time,mon = <0>;
                        init_time,year = <114>;
                        init_time,wday = <3>;

			regulators {
				buck1_reg: BUCK1 {
					regulator-name = "vdd_mif";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1600000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				buck2_reg: BUCK2 {
					regulator-name = "vdd_arm";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1600000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				buck3_reg: BUCK3 {
					regulator-name = "vdd_int";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1600000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo13_reg: LDO13 {
					regulator-name = "vdd_zb_2.8";
					regulator-min-microvolt = <2400000>;
					regulator-max-microvolt = <2400000>;
					regulator-initial-mode = <1>;
				};

				ldo16_reg: LDO16 {
					regulator-name = "vdd_lcd_3.3";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3375000>;
					regulator-initial-mode = <1>;
				};

				ldo18_reg: LDO18 {
					regulator-name = "vcc_peri_device_2.8";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3375000>;
					regulator-initial-mode = <1>;
				};

				ldo19_reg: LDO19 {
					regulator-name = "vcc_avdd_1.8";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <2375000>;
					regulator-initial-mode = <1>;
				};

				ldo20_reg: LDO20 {
					regulator-name = "vcc_peri_device_1.8";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <2375000>;
					regulator-initial-mode = <1>;
				};

				ldo21_reg: LDO21 {
					regulator-name = "zbcore_1.25";
					regulator-min-microvolt = <1250000>;
					regulator-max-microvolt = <1250000>;
					regulator-initial-mode = <1>;
				};

				ldo23_reg: LDO23 {
					regulator-name = "vcc_peri_1.8";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <2375000>;
					regulator-always-on;
				};

				ldo6_reg: LDO6 {
                                        regulator-name = "VDD_USB_AP_1.0V";
                                        regulator-min-microvolt = <1000000>;
                                        regulator-max-microvolt = <1000000>;
                                        regulator-always-on;
					regulator-initial-mode = <1>;
                                };

				ldo8_reg: LDO8 {
                                        regulator-name = "VDD_UOTG_AP_3.3V";
                                        regulator-min-microvolt = <3000000>;
                                        regulator-max-microvolt = <3000000>;
                                };

				ldo2_reg: LDO2 {
					regulator-always-on;
					regulator-initial-mode = <1>;
                                };

				ldo4_reg: LDO4 {
					regulator-always-on;
                                        regulator-initial-mode = <1>;
                                };

				ldo5_reg: LDO5 {
					regulator-always-on;
                                        regulator-initial-mode = <1>;
                                };

				ldo7_reg: LDO7 {
					regulator-always-on;
                                        regulator-initial-mode = <1>;
                                };
			};
		};
	};

	dwmmc0@12510000 {
		status = "okay";
		num-slots = <1>;
		broken-cd;
		bypass-smu;
		fixed_volt;
		only_once_tune;
		extra_tuning;
		supports-highspeed;
		enable-cache-control;
		sw_data_timeout;
		fifo-depth = <0x80>;
		card-detect-delay = <200>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 4 2>;
		samsung,dw-mshc-hs200-timing = <3 0 2 0>;
		samsung,dw-mshc-ddr200-timing = <1 1 2 0>;
		num-ref-clks = <8>;
		gpios = <&gpk0 2 0x1>;
		ciu_clkin = <25 50 50 100 200 100 200 200>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_bus1 &sd0_bus4 &sd0_bus8>;

		slot@0 {
			reg = <0>;
			bus-width = <8>;
		};
	};

	dwmmc1@12520000 {
		status = "okay";
		num-slots = <1>;
		channel = <1>;
		caps-control;
		bypass-smu;
		clock-gate;
		enable-cclk-on-suspend;
		fixed_volt;
		use-fine-tuning;
		supports-highspeed;
		sw_data_timeout;
		keep-power-in-suspend;
		pm-ignore-notify;
		cd-type-external;
		cd-type = <2>;
		use-broken-voltage;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		qos_int_level = <222000>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 3 0>;
		samsung,dw-mshc-ddr-timing = <3 0 2 0>;
		clk_pin = "gpk1-0";
		clk_addr = "11000000.pinctrl";
		clk_val = <0x3>;
		clk_str_num = <4>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd1_clk &sd1_cmd &sd1_bus1 &sd1_bus4>;

		slot@0 {
			reg = <0>;
			bus-width = <4>;
			disable-wp;
		};
	};

	dwmmc2@12530000 {
		status = "okay";
		num-slots = <1>;
		bypass-smu;
		supports-highspeed;
		fixed_volt;
		cd-type = <1>;
		fifo-depth = <0x80>;
		card-detect-delay = <200>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 4 2>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 200>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd2_clk &sd2_cmd &sd2_cd &sd2_bus1 &sd2_bus4>;

		slot@0 {
			reg = <0>;
			bus-width = <4>;
			disable-wp;
		};
	};

        ion {
                compatible = "samsung,exynos5430-ion";

		contig-names = "mfc_sh",
				"video",
				"mfc_fw",
				"mfc_nfw";

		contig = <2 0x100000 0x0>,
			<11 0xA00000 0x0>,
			<8 0x100000 0x0>,
			<12 0x100000 0x0>;

                ion_noncontig_heap {
                        id-type = <0 0>;
                };

		exynos_contig_heap {
			id-type = <4 6>;
		};

        };

	i2c@13870000 {
	        samsung,i2c-sda-delay = <25>;
	        samsung,i2c-max-bus-freq = <400000>;
	        #address-cells = <1>;
	        #size-cells = <0>;
	        status = "okay";

	        CW2015@62 {
	                compatible = "cw201x";
	                reg = <0x62>;
	                /*      pinctrl-0 = <&cw_irq>; */
	                bat_config_info = <0x17 0x67 0x6C 0x70 0x70 0x6F 0x6A 0x65 0x62 0x5B
	                                   0x55 0x4F 0x4A 0x46 0x43 0x3D 0x35 0x2B 0x26 0x22
	                                   0x21 0x2B 0x42 0x4D 0x49 0x49 0x0B 0x85 0x09 0x12
	                                   0x1D 0x3A 0x47 0x44 0x43 0x4B 0x39 0x17 0x37 0x48
	                                   0x04 0x16 0x52 0x87 0x8F 0x91 0x94 0x52 0x82 0x8C
	                                   0x92 0x96 0x81 0x6B 0xC0 0xCB 0x2F 0x7D 0x72 0xA5
	                                   0xB5 0xC1 0x6A 0x09>;
	                /*
	                 is_dc_charge = <1>;
	                 dc_det_gpio = <1>;
	                 is_usb_charge = <0>;
	                */
	        };
	};

	sysreg_localout: sysreg_localout@10010000 {
		compatible = "samsung,exynos5-sysreg_localout";
		reg = <0x10010000 0x2000>;
	};

	mdev_0: mdev_output {
		compatible = "samsung,exynos5-mdev";
	};

	usb@12480000 {
		vusb_d-supply = <&ldo6_reg>;
		vusb_a-supply = <&ldo8_reg>;
		status = "okay";
	};

	slp_multi: slp_multi {
		compatible = "slp,slp_multi";
		slp,slp_mult-nluns = <2>;
		slp,slp_mult-swfi-latency = <1>;
		slp,slp_mult-nfuncs = <4>;
		slp,slp_mult-enable-funcs = "rndis", "sdb", "mtp", "acm";
	};

	spi@13920000 {
		status = "okay";
			#address-cells = <1>;
			#size-cells = <0>;
		ax88796c@0 {
			compatible = "asix,ax88796c";
			gpios = <&gpx2 0 1>;
			interrupt-parent = <&gpx2>;
			interrupts = <0 0 0>;
			pinctrl-names = "default";
			pinctrl-0 = <&eint16>;
			spi-max-frequency = <40000000>;
			/* swap-mode; */
			/* spi-cpol; */
			/* spi-cpha; */
			reg = <0x0>;
			ax88796c,reset = <&gpe0 2 1>;
			clocks = <&clock CLK_FOUT_MPLL>,
				<&clock CLK_DIV_MPLL_PRE>,
				<&clock CLK_MOUT_SPI0>,
				<&clock CLK_SCLK_SPI0>;
			clock-names = "fout_mpll", "dout_mpll_pre", "mout_spi0", "sclk_spi0";
			controller-data {
				cs-gpio = <&gpx3 4 1>;
				samsung,spi-feedback-delay = <2>;
				samsung,spi-chip-select-mode;
			};
		};
	};

	serial@13800000 {
		status = "okay";
	};

	pinctrl@11000000 {
		cfg_wlanen: cfg-wlanen {
			samsung,pins = "gpx2-3";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <3>;
			samsung,pin-con-pdn = <3>;
			samsung,pin-pud-pdn = <0>;
		};

		cfg_wlanhostwake: cfg-wlanhostwake {
			samsung,pins = "gpx3-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <1>;
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <0>;
		};

		cfg_en: cfg-en {
			samsung,pins ="gpx1-7";
			samsung,pin-function = <1>;
			samsung,pin-pud = <1>;
			samsung,pin-con-pdn = <3>;
			samsung,pin-pud-pdn = <1>;
		};

		cfg_btwake: cfg-btwake {
			samsung,pins = "gpx3-7";
			samsung,pin-function = <1>;
			samsung,pin-pud = <1>;
			samsung,pin-con-pdn = <0>;
			samsung,pin-pud-pdn = <1>;
		};

		cfg_hostwake: cfg-hostwake {
			samsung,pins = "gpx3-6";
			samsung,pin-function = <0>;
			samsung,pin-pud = <3>;
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <0>;
		};

		zb_rstn: zb-rstn {
			samsung,pins = "gpm0-1";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-val = <1>;
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <1>;
		};

		zb_bootloader: zb-bootloader {
			samsung,pins = "gpe0-0";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-val = <1>;
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <1>;
		};
	};

	wlan {
		compatible = "samsung,brcm-wlan";
		gpios = <&gpx2 3 0x1	/* WLAN_EN */
			 &gpx3 2 0xF	/* WLAN_HOST_WAKE */
			>;
		pinctrl-names = "default";
		pinctrl-0=<&cfg_wlanen &cfg_wlanhostwake>;
		status = "okay";
	};

	serial@13800000 {
		force-auto-flow-control;
	};

	bluetooth {
		compatible = "broadcom,bcm4354bt";

		gpios = <&gpx1 7 0	/* BT_EN */
			 &gpx3 7 0	/* BT_WAKE */
			 &gpx3 6 0xF	/* BT_HOST_WAKE */
			 >;

		pinctrl-names = "default";
		pinctrl-0=<&cfg_hostwake &cfg_btwake &cfg_en>;
		status = "okay";
	};

	gpio_keys {
		compatible = "gpio-keys";
		/*Temporary changing for ARTIK to exit App using power key*/
		power_key {
			label = "back key";
			linux,code = <158>;
			gpios = <&gpx2 7 0x1>;
			gpios-key,wakeup = <1>;
			debounce-interval = <0>;
		};
		/*
		Original Power key mapping code
		power_key {
			label = "power key";
			linux,code = <116>;
			gpios = <&gpx2 7 0x1>;
			gpio-key,wakeup = <1>;
			debounce-interval = <0>;
		};
		*/
	};

	i2c@13870000 {
		status = "okay";
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		#size-cells = <0>;

		gaudio_codec: ak4953@13 {
			compatible = "akm,ak4953";
			reg = <0x13>;
			ak4953,pdn-gpio = <&gpc0 3 0x1>;
		};

		bq24296_charger@6b {
			compatible = "ti,bq24296";
			reg = <0x6b>;
			ti,chg_current = <1000 500 2000>;
		};
	};

	i2s: i2s@13970000 {
		status = "okay";
	};

	sound {
		compatible = "samsung,artik-ak4953";
		samsung,audio-cpu = <&i2s>;
		samsung,audio-codec = <&gaudio_codec>;
		clocks = <&clock CLK_MOUT_EPLL>,
			 <&clock CLK_FOUT_EPLL>,
			 <&clock CLK_MOUT_AUDIO>,
			 <&clock CLK_DIV_AUDIO>,
			 <&clock CLK_DIV_I2S>,
			 <&clock CLK_SCLK_I2S>;
		clock-names = "mout_epll", "fout_epll",
			      "mout_audio", "dout_audio",
			      "dout_i2s", "sclk_i2s";
		status = "okay";
	};

	artik_zb_power {
		compatible = "samsung,artik_zb_power_v31";
		pinctrl-names = "default";
		pinctrl-0 = <&zb_rstn &zb_bootloader>;
		vdd_zb-supply = <&ldo13_reg>;
		vdd-supply = <&ldo18_reg>;
		vdd_zbcore-supply = <&ldo21_reg>;
	};

	i2c@13890000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		#size-cells = <0>;
		status = "okay";
	};

	regulators {
                compatible = "simple-bus";
                #address-cells = <1>;
                #size-cells = <0>;

		vddext_18: voltage-regulator-1 {
                        compatible = "regulator-fixed";
			regulator-name = "VDDEXT_18";
                        regulator-min-microvolt = <1800000>;
                        regulator-max-microvolt = <1800000>;
		};

		vddext_28: voltage-regulator-2 {
                        compatible = "regulator-fixed";
			regulator-name = "VDDEXT_28";
                        regulator-min-microvolt = <2800000>;
                        regulator-max-microvolt = <2800000>;
		};
	};

	i2c_gpio0: i2c@2d {
		compatible = "i2c-gpio";
		gpios = <&gpm4 1 0 &gpm4 0 0>;
		i2c-gpio,delay-us = <2>;    /* ~100kHz */
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";

		s5k4ec@56 {
			compatible = "samsung,exynos5-fimc-is-sensor-4ec";
			reg = <0x56>;
		};

		s5k5ea@2d {
			compatible = "samsung,exynos5-fimc-is-sensor-5ea";
			reg = <0x2d>;
		};
	};

	fimc_is@14000000 {
		fixed_sensor_id = <14>;
		fimc_is_dvfs {
			default_int = <134000>; /* L1 */
			default_cam = <320000>; /* L0 */
			default_mif = <400000>; /* L0 */
			default_i2c = <0>;

			rear_preview_int = <134000>; /* L1 */
			rear_preview_cam = <320000>; /* L0 */
			rear_preview_mif = <400000>; /* L0 */
			rear_preview_i2c = <0>;

			rear_capture_int = <134000>; /* L1 */
			rear_capture_cam = <320000>; /* L0 */
			rear_capture_mif = <400000>; /* L0 */
			rear_capture_i2c = <0>;

			rear_camcording_int = <134000>; /* L1 */
			rear_camcording_cam = <320000>; /* L0 */
			rear_camcording_mif = <400000>; /* L0 */
			rear_camcording_i2c = <0>;

			max_int = <134000>; /* L1 */
			max_cam = <320000>; /* L0 */
			max_mif = <400000>; /* L0 */
			max_i2c = <0>;
		};
	};

	fimc_is_sensor0: fimc_is_sensor@120A0000 {
		scenario = <2>;
		gpio_reset = <&gpm3 2 0x1>;
		gpios_cam_en = <&gpm0 2 0x1>;
		id = <0>;
		mclk_ch = <1>;
		csi_ch = <0>;
		flite_ch = <0>;
		i2c_ch = <0>;
		i2c_addr = <0x20>;
		is_bns = <0>;
		status = "okay";
	};
};

&pmic_irq {
	samsung,pins = "gpx3-5";
	samsung,pin-function = <0xf>;
	samsung,pin-pud = <3>;
	samsung,pin-drv = <3>;
};

&pm_wrsti {
	samsung,pins = "gpe2-0";
	samsung,pin-pud = <3>;
	samsung,pin-con-pdn = <3>;
};

&spi_1 {
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;

	spidev@0 {
		compatible = "spidev";
		reg = <0>;
		spi-max-frequency = <40000000>;

		controller-data {
			cs-gpio = <&gpb 5 0>;
			samsung,spi-feedback-delay = <2>;
			samsung,spi-chip-select-mode;
		};
	};
};

&adc {
	status = "okay";
	clock-names = "gate_adcif", "sclk";
	clocks = <&clock CLK_TSADC>, <&clock CLK_SCLK_TSADC>;
};

&sec_pwm {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pwm0_out>, <&pwm1_out>;
};

&i2c_7 {
	status = "okay";
	samsung,sysreg-phandle = <&sys_reg>;
};

&dsi_0 {
	vddcore-supply = <&ldo6_reg>;
	vddio-supply = <&ldo6_reg>;
	samsung,pll-clock-frequency = <24000000>;
	status = "okay";

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@1 {
			reg = <1>;

			dsi_out: endpoint {
				remote-endpoint = <&dsi_in>;
				samsung,burst-clock-frequency = <400000000>;
				samsung,esc-clock-frequency = <7200000>;
			};
		};
	};

	panel@0 {
		compatible = "sparkling,eh400wv";
		reg = <0>;
		vdd3-supply = <&vddext_28>;
		vci-supply = <&vddext_18>;
		reset-gpio = <&gpe2 1 0>;
		psr-te-gpio = <&gpm0 0 0>;
		power-on-delay= <30>;
		power-off-delay= <120>;
		reset-delay = <5>;
		init-delay = <100>;
		flip-horizontal;
		flip-vertical;
		panel-width-mm = <52>;
		panel-height-mm = <86>;

		display-timings {
			timing-0 {
				clock-frequency = <24800400>;
				hactive = <480>;
				vactive = <800>;
				hfront-porch = <16>;
				hback-porch = <16>;
				hsync-len = <2>;
				vfront-porch = <28>;
				vback-porch = <28>;
				vsync-len = <2>;
			};
		};

		port {
			dsi_in: endpoint {
				remote-endpoint = <&dsi_out>;
			};
		};
	};
};

&fimd_drm {
	status = "okay";
};
